<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <ul class='menu'><%= link_to 'home', controller: 'home' %> | <%= link_to 'articles', controller: 'articles' %> | <a href="/contacts">contact</a> | <a href="/about">about</a> | <a href="/terms">terms</a> </ul>

 

    <%= yield %>
  </body>
</html>
